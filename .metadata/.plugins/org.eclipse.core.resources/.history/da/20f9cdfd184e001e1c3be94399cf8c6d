<!DOCTYPE properties SYSTEM "query.dtd">
<properties>
	<entry key="insertMember">
		INSERT INTO MEMBER VALUES(SEQ_USERNO.NEXTVAL,?,?,?,?,?,?,?,?,?,SYSDATE
	</entry>
	<entry key="selectList">
		SELECT * FROM MEMBER ORDER BY AGE
	</entry>
	<entry key="selectByUserId">
		SELECT * FROM MEMBER WHERE USERID= ?
	</entry>
	<entry key="selectByUserName">
		SELECT * FROM MEMBER WHERE USERNAME LIKE ?
	</entry>
	<entry key="updateMember">
		UPDATE MEMBER SET USERPWD = ?,EMAIL=?, PHONE=?, ADDRESS=? WHERE USERNAME=?
	</entry>
	<entry key="deleteMember">
		DELETE FROM MEMBER WHERE USERID =?
	</entry>
</properties>
